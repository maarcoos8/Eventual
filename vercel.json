{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/eventos/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/session-logs",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/docs",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/openapi.json",
      "dest": "/api/index.py"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|json|woff|woff2|ttf|eot))",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
